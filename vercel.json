{
    "version": 2,
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "functions": {
    "/server.js": {
      "maxDuration": 300
    }
    
  }
}